{
  "name": "@circle/prisma",
  "version": "1.0.0",
  "type": "commonjs",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "author": "GakuruIan",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "default": "./dist/index.js"
    }
  },
  "files": [
    "dist/**/*",
    "prisma/**/*"
  ],
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "db:generate": "prisma generate",
    "db:push": "prisma db push",
    "db:migrate": "prisma migrate dev",
    "db:studio": "prisma studio",
    "clean": "rm -rf dist",
    "outDir": "./dist",
    "rootDir": "./src"
  },
  "devDependencies": {
    "prisma": "^6.10.1",
    "typescript": "^5.0.0"
  },
  "dependencies": {
    "@prisma/client": "^6.10.1",
    "@prisma/extension-accelerate": "^2.0.1"
  }
}
